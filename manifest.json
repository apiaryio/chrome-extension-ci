{
  "manifest_version": 2,
  "name": "Chrome Extension Continuous Integration",
  "version": "0.0.1",
  "externally_connectable": {
    "matches": [
      "http://*.second.first:3000/*",
      "https://*.second.first:3000/*"
    ]
  },

  "background": {
    "scripts": [
      "js/build.js",
      "js/background.js"
    ],
    "persistent": false
  },

  "permissions": [
    "http://*/*"
  ]
}